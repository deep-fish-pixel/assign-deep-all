{
  "name": "assign-deep-all",
  "version": "1.1.1",
  "description": "assign or extends object、array、set、map、date、regExp by deep or shallow",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/test/test.js",
    "debug": "node --inspect-brk build/test/test01.js",
    "clean": "rimraf build",
    "build": "npm run clean && babel src --out-dir build",
    "build-dev": "babel src --out-dir build --source-maps",
    "test": "npm run test:unit",
    "test:unit": "cross-env NODE_ENV=test mocha ./test/**/*.test.js --compilers js:babel-register",
    "test-debug": "cross-env NODE_ENV=test mocha ./test/**/*.test.js --inspect-brk --compilers js:babel-register"
  },
  "keywords": [
    "assign",
    "extends",
    "object",
    "deep",
    "shallow"
  ],
  "author": "maweimaweima",
  "license": "ISC",
  "dependencies": {
    "clone": "^2.1.1",
    "clone-deep": "^2.0.1",
    "invariant": "2.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "babel-runtime": "^6.26.0",
    "chai": "^3.0.0 || ^4.0.0",
    "cross-env": "^3.1.4",
    "mocha": "^3.0.0",
    "rimraf": "^2.6.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maweimaweima/assign-deep-all.git"
  }
}
